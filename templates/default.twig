<head>
    <style>
        {{ style }}
    </style>
    <meta charset="UTF-8">
</head>
<body>
<div class="row resume">
    <div class="col l4 m4 s4 sidebar center-align">
        <div class="row">
            <div class="col s12">
                <img src="{{ data.pictureUrls.values[0] }}" class="responsive-img circle sidebar__avatar">
            </div>
            <div class="col s12">
                <h4 class="sidebar__name">
                    {{ data.firstName }} {{ data.lastName }}
                </h4>
            </div>
            <div class="col s12">
                <h5 class="sidebar__headline">
                    {{ data.headline }}
                </h5>
            </div>
        </div>
        <div class="row left-align">
            <div class="col s12">
                <h5>CONTACT ME</h5>
            </div>
        </div>
    </div>
    <div class="col l8 m8 s8 content">
        Độc lập tự do hạnh phúc
    </div>
</div>
</body>